IF
	Global("CleanseVampire","AR1401",2)
	Global("VP_KeldornVampire","GLOBAL",3)
	Name("Keldorn",Myself)
	AreaCheck("AR1401")
THEN
	RESPONSE #100
		SetGlobal("VP_KeldornVampire","GLOBAL",4)
		DialogInterrupt(TRUE)
		ChangeAIScript("Keldorn",OVERRIDE)
END

IF
	Global("VP_KeldornVampire","GLOBAL",1)
	Global("RomanceVampire","GLOBAL",0)
THEN
	RESPONSE #100
		SetGlobal("RomanceVampire","GLOBAL",1)
		Continue()
END

IF
	Global("VP_KeldornVampire","GLOBAL",1)
	Global("RomanceVampire","GLOBAL",1)
	Global("RomanceVampireTeleport","GLOBAL",0)
THEN
	RESPONSE #100
		SetGlobal("RomanceVampireTeleport","GLOBAL",1)
		CreateVisualEffectObject("SPDIMNDR",Myself)
		Wait(2)
		DestroySelf()
END